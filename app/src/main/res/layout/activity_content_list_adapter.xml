<android.support.v7.widget.CardView
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/all_store"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginRight="5dp"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="20dp">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/RLdetailDate"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ivTopPhoto"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/icglobe"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/fa_globe"
                android:textColor="@color/bgWhite"
                android:textSize="@dimen/icon_size"
                android:gravity="center"
                android:includeFontPadding="false"
                android:layout_marginRight="@dimen/between_tv_margin"
                android:layout_marginLeft="@dimen/most_left_tv_margin"/>
            <TextView
                android:id="@+id/tvContentInsertDate"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="2016.06.20"
                android:textColor="@color/bgWhite"
                android:textSize="@dimen/content_text_size"
                android:gravity="center"
                android:includeFontPadding="false"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LLmenu"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_alignTop="@+id/ivTopPhoto"
            android:layout_alignParentTop="true"
            android:layout_alignEnd="@+id/ivTopPhoto"
            android:gravity="center">
            <TextView
                android:id="@+id/btnDetailMenu"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/bgNo"
                android:text="@string/fa_ellipsis_v"
                android:textSize="@dimen/icon_size"
                android:textColor="@color/bgWhite"
                android:gravity="right|center"
                android:layout_marginRight="@dimen/most_rignt_tv_margin"/>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/RldetailData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="60dp">
            <ImageView
                android:id="@+id/icline"
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:src="@drawable/ic_line"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:id="@+id/tvContentSubject"
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:text="영국으로의 여행"
                android:textSize="@dimen/content_text_size"
                android:textColor="@color/bgWhite"
                android:layout_below="@+id/icline"
                android:layout_centerHorizontal="true"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:orientation="horizontal"
                android:layout_below="@+id/tvContentSubject"
                android:layout_centerHorizontal="true">
                <TextView
                    android:id="@+id/icmarker"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/fa_icon_marker"
                    android:textSize="@dimen/icon_size"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:textColor="@color/textPurple"
                    android:layout_marginRight="@dimen/between_tv_margin"/>
                <TextView
                    android:id="@+id/tvContentLocation"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="서울시 마포구"
                    android:textSize="@dimen/content_text_size"
                    android:includeFontPadding="false"
                    android:textColor="@color/bgWhite"
                    android:gravity="center"/>
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:id="@+id/ivTopPhoto"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:orientation="vertical"
            android:background="@drawable/xml_border2"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:scaleType="centerCrop">
        </ImageView>
        <LinearLayout
            android:id="@+id/lldetailSetting"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_above="@+id/lldetailInfo"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@color/bgToMeong"
            android:gravity="center">
            <LinearLayout
                android:id="@+id/btnLookLeft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/most_left_tv_margin">
                <TextView
                    android:id="@+id/icleft"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/fa_photo"
                    android:textSize="@dimen/icon_size"
                    android:textColor="@color/bgWhite"
                    android:includeFontPadding="false"
                    android:gravity="center_vertical"
                    android:layout_marginRight="@dimen/between_tv_margin"/>
                <TextView
                    android:id="@+id/tvleft"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@color/bgNo"
                    android:text="사진으로 보기"
                    android:textSize="@dimen/content_text_size"
                    android:textColor="@color/bgWhite"
                    android:includeFontPadding="false"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="right"
                android:layout_marginRight="@dimen/most_rignt_tv_margin">
                <LinearLayout
                    android:id="@+id/btnLookRight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/icright"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/fa_video_camera"
                        android:textSize="@dimen/icon_size"
                        android:textColor="@color/bgWhite"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:layout_marginRight="@dimen/between_tv_margin"/>
                    <TextView
                        android:id="@+id/tvright"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@color/bgNo"
                        android:text="슬라이드로 보기"
                        android:textSize="@dimen/content_text_size"
                        android:textColor="@color/bgWhite"
                        android:layout_gravity="center"
                        android:gravity="center_vertical"
                        android:includeFontPadding="false" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lldetailInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="3"
            android:background="@drawable/xml_border"
            android:layout_below="@+id/ivTopPhoto"
            android:gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="10dp">
                <com.mikhaellopez.circularimageview.CircularImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_profile_circle"
                    android:onClick="ProFileMenuClick"
                    app:civ_border_width="1dp"
                    android:layout_marginRight="@dimen/between_tv_margin"/>
                <TextView
                    android:id="@+id/tvUsername"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="hara jeon"
                    android:textSize="20dp"
                    android:textColor="@color/bgDarkGrey"
                    android:includeFontPadding="false"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_marginBottom="10dp">
                <TextView
                    android:id="@+id/iccal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/fa_calendar"
                    android:textSize="@dimen/icon_size"
                    android:textColor="@color/bgDarkGrey"
                    android:includeFontPadding="false"
                    android:gravity="center"
                    android:layout_gravity="center"/>
                <TextView
                    android:id="@+id/tvDuring"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.3"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/bgDarkGrey"
                    android:text="2016.04.20 ~ 2016.06.20"
                    android:textSize="@dimen/content_text_size"
                    android:includeFontPadding="false"
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"/>
                <TextView
                    android:id="@+id/tvdetailcount"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.3"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/textBlue"
                    android:text="10"
                    android:textSize="@dimen/content_text_size"
                    android:includeFontPadding="false"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.3"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/textBlue"
                    android:text="pic"
                    android:textSize="@dimen/content_text_size"
                    android:includeFontPadding="false"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lldetailData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lldetailInfo"
            android:orientation="vertical"
            android:background="@drawable/xml_border">
            <LinearLayout
                android:id="@+id/lldetailDataCount"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/lldetailInfo">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0.9"
                    android:gravity="left">
                    <TextView
                        android:id="@+id/icpow"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/fa_bookmark_o"
                        android:textSize="@dimen/icon_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center_vertical"
                        android:layout_gravity="center"
                        android:layout_marginLeft="10dp"
                        android:includeFontPadding="false"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="0.8"
                        android:gravity="center">
                        <TextView
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:text="좋아요"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="right"
                            android:layout_gravity="center"
                            android:layout_marginRight="@dimen/between_tv_margin"
                            android:includeFontPadding="false"/>
                        <TextView
                            android:id="@+id/tvLikeCount"
                            android:layout_width="30dp"
                            android:layout_height="wrap_content"
                            android:text="13"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:gravity="center">
                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="wrap_content"
                            android:text="댓글"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="right"
                            android:layout_gravity="center"
                            android:layout_marginRight="@dimen/between_tv_margin"
                            android:includeFontPadding="false"/>
                        <TextView
                            android:id="@+id/tvCommentCount"
                            android:layout_width="30dp"
                            android:layout_height="wrap_content"
                            android:text="13"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:layout_marginRight="@dimen/most_rignt_tv_margin">
                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="wrap_content"
                            android:text="공유"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="right"
                            android:layout_gravity="center"
                            android:layout_marginRight="@dimen/between_tv_margin"
                            android:includeFontPadding="false"/>
                        <TextView
                            android:id="@+id/tvShareCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="13"
                            android:textSize="@dimen/content_text_size"
                            android:textColor="@color/bgDarkGrey"
                            android:gravity="center_vertical|right"
                            android:layout_gravity="center"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/lldetailDataCount">
                <LinearLayout
                    android:id="@+id/btnLike"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="left">
                    <TextView
                        android:id="@+id/icthumb"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/fa_thumbs_o_up"
                        android:textSize="@dimen/icon_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/most_left_tv_margin"
                        android:layout_marginRight="@dimen/between_tv_margin"
                        android:includeFontPadding="false"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@color/bgNo"
                        android:text="좋아요"
                        android:textSize="@dimen/content_text_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center_vertical"
                        android:layout_gravity="center"
                        android:includeFontPadding="false"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center">
                    <TextView
                        android:id="@+id/iccomment"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/fa_comments"
                        android:textSize="@dimen/icon_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/between_tv_margin"
                        android:includeFontPadding="false"/>
                    <TextView
                        android:id="@+id/btnComment"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@color/bgNo"
                        android:text="댓글쓰기"
                        android:textSize="@dimen/content_text_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center_vertical"
                        android:layout_gravity="center"
                        android:includeFontPadding="false"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/btnShare"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right">
                    <TextView
                        android:id="@+id/icshare"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/fa_share"
                        android:textSize="@dimen/icon_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/between_tv_margin"
                        android:includeFontPadding="false"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@color/bgNo"
                        android:text="공유하기"
                        android:textSize="@dimen/content_text_size"
                        android:textColor="@color/bgDarkGrey"
                        android:gravity="center_vertical|right"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/most_rignt_tv_margin"
                        android:includeFontPadding="false"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</android.support.v7.widget.CardView>