<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_main"
    tools:context="com.kitri.photovel.MainActivity">

    <Button
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/clusterTest"
        android:text="clusterTest" />

    <Button
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/contentInsert"
        android:text="contentInsert" />

    <Button
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/contentUpdate"
        android:text="contentUpdate" />

    <Button
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/photoTest"
        android:text="contentUpdate" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/slideShowTest"
        android:text="slideShowTest"/>

</LinearLayout>
